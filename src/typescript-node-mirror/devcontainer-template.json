  "options": {
    "imageVariant": {
      "type": "string",
      "description": "Node.js version (use -bookworm, -bullseye variants on local arm64/Apple Silicon):",
      "proposals": [
        "20-bookworm",
        "18-bookworm",
        "16-bookworm",
        "20-bullseye",
        "18-bullseye",
        "16-bullseye",
        "20-buster",
        "18-buster",
        "16-buster"
      ],
      "default": "20-bullseye"
    }
  },
  "platforms": ["Node.js", "TypeScript"]
}
